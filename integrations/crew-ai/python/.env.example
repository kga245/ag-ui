# CrewAI Integration (uses litellm - supports multiple providers)
# Required: At least one LLM provider API key

# OpenAI (Primary - used by default in examples with model="openai/gpt-4o")
OPENAI_API_KEY=

# Optional: Alternative LLM Providers (change model= in examples to use)
# litellm supports: openai/, anthropic/, gemini/, azure/, etc.
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=
AZURE_API_KEY=
AZURE_API_BASE=

# Optional: Tool APIs
TAVILY_API_KEY=
SERPER_API_KEY=

# Server Configuration
PORT=9002
